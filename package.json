{
  "name": "onesignal-api-client-core",
  "version": "1.1.2",
  "description": "A Typescript Library for OneSignal push notification service",
  "author": "kvandake",
  "license": "MIT",
  "keywords": [
    "OneSignal",
    "Typescript",
    "Axios",
    "NodeJS"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/kvandake/onesignal"
  },
  "bugs": {
    "url": "https://github.com/kvandake/onesignal/issues"
  },
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "clean": "rimraf lib",
    "build": "yarn run clean && tsc",
    "test": "jest",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "yarn run build",
    "prepublishOnly": "yarn test && yarn run lint",
    "preversion": "yarn run lint",
    "version": "yarn run format && git add -A",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.19.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.5.3"
  },
  "files": [
    "lib/**/*"
  ]
}
